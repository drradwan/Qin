<QinIM>
  <name>Symbols</name>
  <tableim>false</tableim>
  <preeditable>false</preeditable>
  <customkeymap>true</customkeymap>
  <keymap>
    <normal>
      <key value="q">1</key>
      <key value="w">2</key>
      <key value="e">3</key>
      <key value="r">4</key>
      <key value="t">5</key>
      <key value="y">6</key>
      <key value="u">7</key>
      <key value="i">8</key>
      <key value="o">9</key>
      <key value="p">0</key>

      <key value="a">-</key>
      <key value="s">=</key>
      <key value="d">+</key>
      <key value="f">'</key>
      <key value="g">"</key>
      <key value="h">(</key>
      <key value="j">)</key>
      <key value="k">&amp;</key>
      <key value="l">@</key>

      <key value="z">/</key>
      <key value="x">;</key>
      <key value="c">:</key>
      <key value="v">_</key>
      <key value="b">!</key>
      <key value="n">,</key>
      <key value="m">.</key>
    </normal>
    <shift>
      <key value="q">[</key>
      <key value="w">]</key>
      <key value="e">{</key>
      <key value="r">}</key>
      <key value="t">#</key>
      <key value="y">^</key>
      <key value="u">*</key>
      <key value="i">•</key>
      <key value="o">°</key>
      <key value="p">%</key>

      <key value="a">?</key>
      <key value="s">$</key>
      <key value="d">|</key>
      <key value="f">'</key>
      <key value="g">"</key>
      <key value="h">&lt;</key>
      <key value="j">&gt;</key>
      <key value="k">~</key>
      <key value="l">@</key>

      <key value="z">\</key>
      <key value="x">;</key>
      <key value="c">:</key>
      <key value="v">¿</key>
      <key value="b">¡</key>
      <key value="n">,</key>
      <key value="m">.</key>
    </shift>
  </keymap>
</QinIM>
