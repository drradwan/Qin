<QinIM>
  <name>Int'l</name>
  <tableim>true</tableim>
  <preeditable>true</preeditable>
  <customkeymap>false</customkeymap>
  <database>/opt/vsd/latin.db</database>
  <maxkeystrokes>1</maxkeystrokes>
  <querytemplate>SELECT phrase FROM phrases WHERE %1 ORDER BY -freq;</querytemplate>
  <keytransform>
    <key value="a">97</key>
    <key value="b">98</key>
    <key value="c">99</key>
    <key value="d">100</key>
    <key value="e">101</key>
    <key value="f">102</key>
    <key value="g">103</key>
    <key value="h">104</key>
    <key value="i">105</key>
    <key value="j">106</key>
    <key value="k">107</key>
    <key value="l">108</key>
    <key value="m">109</key>
    <key value="n">110</key>
    <key value="o">111</key>
    <key value="p">112</key>
    <key value="q">113</key>
    <key value="r">114</key>
    <key value="s">115</key>
    <key value="t">116</key>
    <key value="u">117</key>
    <key value="v">118</key>
    <key value="w">119</key>
    <key value="x">120</key>
    <key value="y">121</key>
    <key value="z">122</key>
  </keytransform>
</QinIM>
